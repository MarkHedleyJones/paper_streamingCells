doi = {10.1016/j.colsurfa.2011.06.014},
file = {:home/mark/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhao, Yang - 2011 - On the competition between streaming potential effect and hydrodynamic slip effect in pressure-driven microchannel f.pdf:pdf},
issn = {09277757},
journal = {Colloids and Surfaces A: Physicochemical and Engineering Aspects},
keywords = {Streaming potential Hydrodynamic slippage Electrov},
month = aug,
