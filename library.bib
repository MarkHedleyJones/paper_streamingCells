journal = {Langmuir : the ACS journal of surfaces and colloids},
month = jun,
number = {11},
pages = {8161--73},
pmid = {20192159},
title = {{Electrofluidic gating of a chemically reactive surface.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20192159},
volume = {26},
year = {2010}
}
