@article{VanderHeyden2006a,
abstract = {We theoretically evaluate the prospect of using electrokinetic phenomena to convert hydrostatic energy to electrical power. An expression is derived for the energy conversion efficiency of a two-terminal fluidic device in terms of its linear electrokinetic response properties. For a slitlike nanochannel of constant surface charge density, we predict that the maximum energy conversion efficiency occurs at low salt concentrations. An analytic expression for the regime of strong double-layer overlap reveals that the efficiency depends only on the ratio of the channel height to the Gouy-Chapman length, and the product of the viscosity and the counterion mobility. We estimate that an electrokinetic energy conversion device could achieve a maximum efficiency of 12\% for simple monovalent ions in aqueous solution.},
author = {van der Heyden, Frank H J and Bonthuis, Douwe Jan and Stein, Derek and Meyer, Christine and Dekker, Cees},
doi = {10.1021/nl061524l},
file = {:home/mark/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/van der Heyden et al. - 2006 - Electrokinetic energy conversion efficiency in nanofluidic channels.pdf:pdf},
issn = {1530-6984},
journal = {Nano letters},
keywords = {Computer Simulation,Computer-Aided Design,Electric Power Supplies,Electricity,Energy Transfer,Equipment Design,Equipment Failure Analysis,Kinetics,Microfluidics,Microfluidics: instrumentation,Microfluidics: methods,Models, Theoretical,Nanotechnology,Nanotechnology: instrumentation,Nanotechnology: methods,Transducers},
month = oct,
number = {10},
pages = {2232--7},
pmid = {17034089},
title = {{Electrokinetic energy conversion efficiency in nanofluidic channels.}},
